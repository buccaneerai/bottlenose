  "description": "Parse and write CSV with RxJS",
  "keywords": [
    "rxjs",
    "bottlenose",
    "csv"
  ],
  "author": "Buccaneer LLC <opensource@buccaneer.ai>",
  "homepage": "https://github.com/buccaneerai/bottlenose#readme",
  "license": "MIT",
  "main": "src/index.js",
  "directories": {
    "src": "src"
  },
  "files": [
    "build"
  ],
  "publishConfig": {
